#! /bin/sh

if [ "$help" != "1" ]
then

	echo -e "#include <frei0r.h>\n int main(){ f0r_plugin_info_t test; test.name;return 0;}\n"| gcc $CFLAGS -c -o test1 -x c -  >/dev/null 2>&1

	if [ "$?" = "1" ]
	then
		touch ../disable-frei0r
		echo "- frei0r plugin disabled. Install frei0r-plugins and make sure frei0r.h is available." 
	fi

fi
